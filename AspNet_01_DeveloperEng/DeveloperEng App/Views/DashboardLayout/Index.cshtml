<!DOCTYPE html>
<html lang="en">

@await Component.InvokeAsync("DashboardLayout", new { partialViewName = "HeadPartial" })

<body class="g-sidenav-show  bg-gray-100">

  @await Component.InvokeAsync("DashboardLayout", new { partialViewName = "LeftSideBarPartial" })

  
  <main class="main-content position-relative max-height-vh-100 h-100 border-radius-lg ">

    @await Component.InvokeAsync("DashboardLayout", new { partialViewName = "NavbarPartial" })

    
    <div class="container-fluid py-4">

      @RenderBody()
      
      @await Component.InvokeAsync("DashboardLayout", new { partialViewName = "FooterPartial" })

    </div>
  </main>

    @await Component.InvokeAsync("DashboardLayout", new { partialViewName = "ConfiguratorPartial" })
    @await Component.InvokeAsync("DashboardLayout", new { partialViewName = "JSPartial" })

</body>

</html>
